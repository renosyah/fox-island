[application]

config/name="Fox Island"
config/description="Is game about surviving as fox in random island"
run/main_scene="res://menu/main-menu/main_menu.tscn"
config/icon="res://icn.png"

[autoload]

Network="*res://addons/network/Network.gd"
Global="*res://global/global.gd"
NetworkLobbyManager="*res://addons/network/lobby-manager/NetworkLobbyManager.gd"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/filters/anisotropic_filter_level=2
gles2/compatibility/disable_half_float=true
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
